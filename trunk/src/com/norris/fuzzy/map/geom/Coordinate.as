	public class Coordinate {
		
		public var x:Number;
		public var y:Number;
		public var z:Number;
		
		public function Coordinate(x:Number = 0, y:Number = 0, z:Number = 0):void {
			this.x = x;
			this.y = y;
			this.z = z;
		}
		
	}
	
